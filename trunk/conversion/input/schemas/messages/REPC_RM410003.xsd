                                    isWidthAutoSize="true"
                                    isHeightAutoSize="true">
                     <mif:position x="15.400" y="4.160"/>
                     <mif:size width="3.202" height="0.781"/>
                  </mif:graphElement>
               </mif:graphicRepresentation>
               <mif:annotations>
                  <mif:definition>
                     <mif:text/>
                  </mif:definition>
               </mif:annotations>
            </mif:class>
         </xs:appinfo>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="InfrastructureRootElements"/>
         <xs:element name="code" type="CV" minOccurs="1" maxOccurs="1">
            <xs:annotation>
               <xs:appinfo>
                  <mif:attribute xmlns:mif="urn:hl7-org:v3/mif" name="code" sortKey="3" minimumMultiplicity="1"
                                 maximumMultiplicity="1"
                                 conformance="R"
                                 isMandatory="false"
                                 isStructural="false">
                     <mif:businessName name="J:Observation Categories"/>
                     <mif:annotations>
                        <mif:definition>
                           <mif:text>
                              <mif:p>Describes the categorization of the observation. E.g. markers of disease progression, co-morbidities / complications etc.</mif:p>
                           </mif:text>
                        </mif:definition>
                        <mif:rationale>
                           <mif:text>
                              <mif:p>Allows categorizing of observations for presentation. A given person may have had numerous observations related to a particular area. By associating categories, a person viewing the EHR information can first look at a list of categories and then drill down to the specific observation.</mif:p>
                              <mif:p>The presence of this field is essential to prevent users from being overwhelmed, however not all observations will necessarily be categorizable. Therefore this element is marked as 'populated'.</mif:p>
                           </mif:text>
                        </mif:rationale>
                     </mif:annotations>
                     <mif:derivationSupplier staticModelDerivationId="1" className="WorkingList" attributeName="code"/>
                     <mif:type name="CV"/>
                     <mif:supplierDomainSpecification codingStrength="CNE" domainName="ActNonLabDIMeasuredObservationCategoryList"/>
                  </mif:attribute>
               </xs:appinfo>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attributeGroup ref="InfrastructureRootAttributes"/>
      <xs:attribute name="classCode" type="ActClass" use="optional" fixed="LIST"/>
      <xs:attribute name="moodCode" type="ActMood" use="optional" fixed="EVN"/>
   </xs:complexType>
</xs:schema>