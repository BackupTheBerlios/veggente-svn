                                 minimumMultiplicity="0"
                                 maximumMultiplicity="*"
                                 isMandatory="false"
                                 isStructural="false">
                     <mif:businessName name="C: Participant Confidentiality"/>
                     <mif:annotations>
                        <mif:definition>
                           <mif:text>
                              <mif:p>Identifies the confidentiality level associated with the encounter for this specific participant.</mif:p>
                           </mif:text>
                        </mif:definition>
                        <mif:usageNotes>
                           <mif:text>
                              <mif:p>This element should only be specified if the id is referencing a patient, not a provider.</mif:p>
                              <mif:p>Stricter confidentiality asserted at the overall encounter level overrides looser confidentiality asserted at the participant level.</mif:p>
                           </mif:text>
                        </mif:usageNotes>
                        <mif:rationale>
                           <mif:text>
                              <mif:p>Provides individual participants in a group encounter with complete control over the confidentiality of their participation in that encounter.</mif:p>
                              <mif:p>The attribute is optional because not all systems will support masking.</mif:p>
                           </mif:text>
                        </mif:rationale>
                     </mif:annotations>
                     <mif:derivationSupplier staticModelDerivationId="1" className="Role"
                                             attributeName="confidentialityCode"/>
                     <mif:type name="SET">
                        <mif:supplierBindingArgumentDatatype name="CV"/>
                     </mif:type>
                     <mif:supplierDomainSpecification codingStrength="CNE" domainName="Confidentiality"/>
                  </mif:attribute>
               </xs:appinfo>
