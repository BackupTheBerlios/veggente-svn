   <owl:ObjectProperty rdf:about="http://veggente.berlios.de/ns/RIMOntology#profileId">
      <dc:identifier>http://veggente.berlios.de/ns/RIMOntology#profileId</dc:identifier>
      <dc:description>The message profile identifier allows a given implementation to explicitly state how it varies from the standard message definition.When multiple profiles are specified, the message instance must be valid against all of them. However, a receiver may choose to validate against only the first one recognized. For this reason, 'preferred' or more-rigorous profiles should be listed first.</dc:description>
      <rdfs:range rdf:resource="http://veggente.berlios.de/ns/RIMDatatype#LIST"/>
      <rdfs:domain rdf:resource="http://veggente.berlios.de/ns/RIMOntology#Message"/>
