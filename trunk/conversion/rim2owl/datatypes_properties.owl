<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
		xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
		xmlns:hl7_dt="http://veggente.berlios.de/ns/RIMDatatype#" 
		xmlns:owl="http://www.w3.org/2002/07/owl#">
		<owl:DatatypeProperty rdf:resource="http://veggente.berlios.de/ns/RIMDatatype#nullFlavor">
				<rdfs:domain rdf:resource="http://veggente.berlios.de/ns/RIMDatatype#ANY"/>
				<rdfs:range rdf:resource=""/>
		</owl:DatatypeProperty>
</rdf:RDF>

